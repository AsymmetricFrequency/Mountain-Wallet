{
  "expo": {
    "userInterfaceStyle": "automatic",
    "name": "CondorWallet",
    "slug": "Condor",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
      
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barneyeldinosaurio.CondorWallet",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "automatic",
      "package": "com.barneyeldinosaurio.CondorWallet",
      "permissions": ["CAMERA", "READ_PHONE_STATE", "RECORD_AUDIO", "READ_CONTACTS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }

  }
}
